##Random Read 4k blocksize test
[global]
ioengine=libaio
size=10GiB
name=fio-rand-read
direct=1
iodepth=128
numjobs=1
group_reporting=1

##----layout file-------
[4k-init-write]
blocksize=4k
filename=/dataset/fio.dat
readwrite=write
end_fsync=1

##----Begin 4k tests-------
[4k-randread]
stonewall
blocksize=4k
filename=/dataset/fio.dat
readwrite=randread
